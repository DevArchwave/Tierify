{
  "schemaVersion": 1,
  "id": "tiered",
  "version": "${version}",
  "name": "Tierify",
  "description": "Adds quality tiers to armor, weapons and tools.",
  "authors": [
    "ElocinDev",
    "Draylar (Tiered)",
    "Globox_Z (TieredZ)"
  ],
  "contact": {
    "issues": "https://github.com/nvb-uy/Tierify/issues",
    "homepage": "https://www.curseforge.com/minecraft/mc-mods/tierify"
  },
  "license": "MIT",
  "icon": "assets/tiered/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "draylar.tiered.Tiered"
    ],
    "client": [
      "draylar.tiered.TieredClient"
    ],
    "modmenu": [
      "draylar.tiered.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "tiered.mixins.json"
  ],
  "depends": {
    "java": ">=17",
    "minecraft": ">=${minecraft_version}",
    "fabric-api": ">=${fabric_version}",
    "fabricloader": ">=${loader_version}",
    "cloth-config2": ">=${cloth_config_version}",
    "necronomicon": ">=${necronomicon_version}"
  }
}